{
  "name": "mobile-app",
  "version": "1.0.1",
  "private": true,
  "description": "React Native Template",
  "license": "MIT",
  "scripts": {
    "android:adb": "adb reverse tcp:3500 tcp:3500",
    "android": "NODE_ENV=development yarn setenv && yarn clean:android && yarn android:adb && ENVFILE=.env.development react-native run-android",
    "android:release": "NODE_ENV=development yarn setenv && yarn clean:android && yarn android:adb && ENVFILE=.env.development react-native run-android --mode release",
    "ios": "yarn pod:reinstall && NODE_ENV=development yarn setenv && ENVFILE=.env.development react-native run-ios --simulator='iPhone 15 Pro Max'",
    "ios:device": "yarn pod:reinstall && NODE_ENV=development yarn setenv && yarn clean:xcode && ENVFILE=.env.development react-native run-ios --device='iPhone 11'",
    "ios:release": "yarn pod:reinstall && NODE_ENV=development yarn setenv && yarn clean:xcode && ENVFILE=.env.development react-native run-ios --mode Release --simulator='iPhone 15 Pro Max'",
    "ios:releasedevice": "yarn pod:reinstall && NODE_ENV=development yarn setenv && yarn clean:xcode && ENVFILE=.env.development react-native run-ios --mode Release --device='iPhone 11'",
    "start": "react-native start",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "e2e:test": "detox test -c ios.sim.debug",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "link:font": "npx react-native-asset",
    "commit": "better-commits",
    "gen:splash": "react-native generate-bootsplash src/assets/splash-logo.png --background=FFFFFF --logo-width=160 --assets-output=src/assets/bootsplash --flavor=main --html=index.html",
    "reset": "yarn clean:module && yarn clean:ios && yarn clean:android && yarn install && yarn reinstall",
    "pod:reinstall": "cd ios/ && pod install && cd ..",
    "pod:forcereinstall": "yarn clean:pod && cd ios/ && pod install && cd ..",
    "clean:cache": "yarn start --reset-cache",
    "clean:module": "rimraf node_modules",
    "clean:xcode": "rimraf ios/build ~/Library/Caches/CocoaPods ~/Library/Developer/Xcode/DerivedData/*",
    "clean:pod": "rimraf ios/Pods ios/Podfile.lock && pod cache clean --all",
    "clean:ios": "yarn clean:pod && yarn clean:xcode",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && rimraf android/build android/.gradle android/app/build",
    "clean:watchman": "watchman watch-del-all",
    "clean:all": "yarn clean:module && yarn clean:watchman && yarn clean:ios && yarn clean:android",
    "doctor": "npx react-native doctor",
    "info": "npx react-native info",
    "bump:major": "npm version major --no-git-tag-version && bundle exec fastlane bump_version",
    "bump:minor": "npm version minor --no-git-tag-version && bundle exec fastlane bump_version",
    "bump:patch": "npm version patch --no-git-tag-version && bundle exec fastlane bump_version",
    "bump:buildnumber": "bundle exec fastlane bump_build_number",
    "postinstall": "patch-package",
    "prepare": "husky",
    "setenv": "node cli/copy.js",
    "setup": "yarn install && cd ios/ && pod install && cd ..",
    "knip": "knip"
  },
