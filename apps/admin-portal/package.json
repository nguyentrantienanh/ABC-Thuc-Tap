  "type": "module",
  "scripts": {
    "app:down": "docker-compose stop app",
    "app:log": "docker-compose logs -f app",
    "app:up": "docker-compose up -d app",
    "build": "tsc -b && vite build",
    "check:format": "prettier --check .",
    "check:types": "tsc --pretty --noEmit",
    "clean": "rimraf .next",
    "commit": "better-commits",
    "dev": "vite --host 0.0.0.0",
    "format": "prettier --write .",
    "postinstall": "patch-package",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "preview": "vite preview --host 0.0.0.0 --port 3001",
    "test": "VITE_CJS_IGNORE_WARNING=true vitest --watch=false",
    "test:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2eui": "playwright test --ui",
    "test:ui": "vitest --ui",
    "test:uicov": "vitest --ui --coverage.enabled",
    "test:watch": "vitest",
    "viewtsconfig": "tsc -p ./tsconfig.json --showConfig",
    "knip": "knip"
  },
