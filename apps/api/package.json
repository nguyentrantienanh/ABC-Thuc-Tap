{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "description": "NestJS - Admin Portal Template",
  "author": {
    "name": "Tin Tran",
    "email": "tindl88@gmail.com",
    "url": "https://github.com/tindl88"
  },
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:clean": "rimraf coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --detectOpenHandles --config ./test/jest-e2e.json",
    "test:e2ecov": "NODE_ENV=test jest --detectOpenHandles --config ./test/jest-e2e.json --coverage",
    "app:down": "docker-compose stop monorepo-template-app",
    "app:log": "docker-compose logs -f monorepo-template-app",
    "app:up": "docker-compose up -d monorepo-template-app",
    "db:down": "docker-compose stop monorepo-template-db",
    "db:log": "docker-compose logs -f monorepo-template-db",
    "db:up": "docker-compose up -d monorepo-template-db",
    "dbtest:down": "docker-compose stop monorepo-template-db-test",
    "dbtest:log": "docker-compose logs -f monorepo-template-db-test",
    "dbtest:up": "docker-compose up -d monorepo-template-db-test",
    "redis:down": "docker-compose stop monorepo-template-redis",
    "redis:log": "docker-compose logs -f monorepo-template-redis",
    "redis:up": "docker-compose up -d monorepo-template-redis",
    "redistest:down": "docker-compose stop monorepo-template-redis-test",
    "redistest:log": "docker-compose logs -f monorepo-template-redis-test",
    "redistest:up": "docker-compose up -d monorepo-template-redis-test",
    "minio:down": "docker-compose stop monorepo-template-minio",
    "minio:log": "docker-compose logs -f monorepo-template-minio",
    "minio:up": "docker-compose up -d monorepo-template-minio",
    "docker:up": "yarn db:up && yarn dbtest:up && yarn minio:up",
    "docker:log": "yarn db:log && yarn dbtest:log && yarn minio:log",
    "docker:down": "yarn db:down && yarn dbtest:down && yarn minio:down",
    "typeorm:ts": "ts-node --project ./tsconfig.json -r tsconfig-paths/register node_modules/.bin/typeorm",
    "typeorm:dropschema": "yarn typeorm:ts schema:drop -d ./src/database/data-source.ts",
    "migration:generate": "yarn typeorm:ts migration:generate -d ./src/database/data-source.ts",
    "migration:create": "yarn typeorm:ts migration:create",
    "migration:run": "yarn typeorm:ts migration:run -d ./src/database/data-source.ts",
    "migration:revert": "yarn typeorm:ts migration:revert -d ./src/database/data-source.ts",
    "migration-test:run": "NODE_ENV=test yarn typeorm:ts migration:run -d ./src/database/data-source.ts",
    "migration-test:revert": "NODE_ENV=test yarn typeorm:ts migration:revert -d ./src/database/data-source.ts",
    "seed:revert": "yarn typeorm:ts migration:revert -d ./src/database/seed-data-source.ts",
    "seed:run": "yarn typeorm:ts migration:run -d ./src/database/seed-data-source.ts",
    "data:init": "yarn typeorm:dropschema && yarn migration:run && yarn seed:run",
    "data:inite2e": "NODE_ENV=test yarn typeorm:dropschema && NODE_ENV=test yarn migration:run",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "format": "prettier --write .",
    "commit": "better-commits",
    "postinstall": "patch-package",
    "prepare": "husky",
    "setup": "yarn docker:up && yarn data:init && yarn data:inite2e",
    "gen:random32": "node -e \"console.log(require('crypto').randomBytes(32).toString('hex'))\"",
    "knip": "knip"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "9.4.3",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "9.4.3",
    "@repo/shared-universal": "*",
    "aws-sdk": "^2.1687.0",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "file-type": "16.5.4",
    "firebase-admin": "^12.1.1",
    "glob": "^11.0.0",
    "google-auth-library": "^9.10.0",
    "helmet": "^7.0.0",
    "isbot": "^5.1.8",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.1.0",
    "libphonenumber-js": "^1.11.17",
    "lodash": "^4.17.21",
    "nestjs-pino": "^4.1.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "patch-package": "^8.0.0",
    "pg": "^8.11.3",
    "pino-http": "^8.3.3",
    "pino-pretty": "^11.2.2",
    "postinstall-postinstall": "^2.1.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sharp": "^0.33.4",
    "slugify": "^1.6.6",
    "socket.io": "4.7.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@repo/config-typescript": "*",
