  "scripts": {
    "dev": "next dev --port 3001",
    "build": "next build",
    "start": "next start --port 3001",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "viewtsconfig": "tsc -p ./tsconfig.json --showConfig",
    "check:types": "tsc --pretty --noEmit",
    "check:format": "prettier --check .",
    "format": "prettier --write .",
    "preview": "next build && next start",
    "test:watch": "vitest",
    "test": "VITE_CJS_IGNORE_WARNING=true vitest --watch=false",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:uicov": "vitest --ui --coverage.enabled",
    "test:e2e": "playwright test",
    "test:e2eui": "playwright test --ui",
    "commit": "better-commits",
    "postinstall": "patch-package",
    "prepare": "husky",
    "clean": "rimraf .next",
    "app:down": "docker-compose stop app",
    "app:log": "docker-compose logs -f app",
    "app:up": "docker-compose up -d app",
    "knip": "knip"
  },
